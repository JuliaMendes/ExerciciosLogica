Algoritmo "Média final escolar"
//  
//  

Var
   n1,n2,n3,media:real


Inicio
   escreval("Escreva a nota 1")
   leia(n1)
   escreval("Escreva a nota 2")
   leia(n2)
   escreval("Escreva a nota 3")
   leia(n3)
   media<- (n1*2+n2*3+n3*5)/10
   escreval("A média do aluno é :",media)




Fimalgoritmo